{
    "template": "nginx-*",
    "index_patterns": [
        "nginx-*"
    ],
    "settings": {
        "number_of_shards": 1
    },
    "mappings": {
        "properties": {
            "remote_addr": {
                "type": "ip"
            },
            "remote_user": {
                "type": "keyword"
            },
            "time_local": {
                "type": "date"
            },
            "request": {
                "type": "keyword"
            },
            "status": {
                "type": "integer"
            },
            "body_bytes_sent": {
                "type": "long"
            },
            "http_referer": {
                "type": "keyword"
            },
            "http_user_agent": {
                "type": "keyword"
            },
            "@timestamp": {
                "type": "date"
            }
        }
    }
}

